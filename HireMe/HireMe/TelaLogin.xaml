<Window x:Class="RockClient.TelaLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RockClient"
        mc:Ignorable="d"
        Title="TelaLogin" Height="300" Width="300">
    <Grid>
        <Label x:Name="labelNumero" Content="Numero" HorizontalAlignment="Left" Margin="28,56,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxNumero" HorizontalAlignment="Left" Height="23" Margin="113,59,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.51,-0.384"/>
        <Label x:Name="labelSenha" Content="Senha" HorizontalAlignment="Left" Margin="28,100,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxSenha" HorizontalAlignment="Left" Height="23" Margin="113,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonLogin" Content="Login" HorizontalAlignment="Left" Margin="161,196,0,0" VerticalAlignment="Top" Width="84" Height="28" Click="buttonLogin_Click"/>

    </Grid>
</Window>
